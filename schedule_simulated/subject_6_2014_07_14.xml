<?xml version="1.0" encoding="UTF-8"?>

<schedule>
  <activity id="Sleeping">
    <times id="1">
      <periodList>
        <period id="1">
          <startTime>00:00</startTime>
          <endTime>05:00</endTime>
        </period>
      </periodList>
      <duration>300</duration>
      <schedulable>false</schedulable>
      <applianceList>
        <appliance id="1">NightLamp</appliance>
      </applianceList>
    </times>
  </activity>
  <activity id="UsingLaptop">
    <times id="1">
      <periodList>
        <period id="1">
          <startTime>17:00</startTime>
          <endTime>20:00</endTime>
        </period>
      </periodList>
      <duration>120</duration>
      <schedulable>true</schedulable>
      <applianceList>
        <appliance id="1">XBOX</appliance>
        <appliance id="2">Radio</appliance>
        <appliance id="3">Fan</appliance>
        <appliance id="4">Light</appliance>
        <appliance id="5">Laptop</appliance>
        <appliance id="6">Microwave</appliance>
        <appliance id="7">VacuumCleaner</appliance>
      </applianceList>
    </times>
  </activity>
  <activity id="DryingClothes">
    <times id="1">
      <periodList>
        <period id="1">
          <startTime>05:00</startTime>
          <endTime>07:00</endTime>
        </period>
        <period id="2">
          <startTime>14:00</startTime>
          <endTime>16:00</endTime>
        </period>
      </periodList>
      <duration>60</duration>
      <schedulable>true</schedulable>
      <applianceList>
        <appliance id="1">Lamp</appliance>
        <appliance id="2">WashingMachine</appliance>
        <appliance id="3">TumbleDryer</appliance>
        <appliance id="4">RiceCooker</appliance>
        <appliance id="5">Microwave</appliance>
      </applianceList>
    </times>
  </activity>
  <activity id="Cleaning">
    <times id="1">
      <periodList>
        <period id="1">
          <startTime>17:00</startTime>
          <endTime>19:00</endTime>
        </period>
      </periodList>
      <duration>120</duration>
      <schedulable>false</schedulable>
      <applianceList>
        <appliance id="1">PC</appliance>
        <appliance id="2">Oven</appliance>
        <appliance id="3">VacuumCleaner</appliance>
      </applianceList>
    </times>
  </activity>
  <activity id="MakingCoffee">
    <times id="1">
      <periodList>
        <period id="1">
          <startTime>10:00</startTime>
          <endTime>11:00</endTime>
        </period>
      </periodList>
      <duration>60</duration>
      <schedulable>false</schedulable>
      <applianceList>
        <appliance id="1">Radio</appliance>
        <appliance id="2">Fan</appliance>
        <appliance id="3">Air-conditioner</appliance>
        <appliance id="4">NightLamp</appliance>
        <appliance id="5">WashingMachine</appliance>
        <appliance id="6">DishWasher</appliance>
        <appliance id="7">CoffeeMaker</appliance>
      </applianceList>
    </times>
  </activity>
</schedule>
